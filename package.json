{
  "name": "techcodes-app",
  "version": "1.0.0",
  "description": "L'app de TechCodes!",
  "main": "index.js",
  "repository": "https://github.com/CodeBTHS/app.git",
  "authors": [
    "Nirjhor Nath",
    "Dashiell Elliot",
    "Santiago Vira"
  ],
  "scripts": {
    "apollo:dev": "dotenv -e .env.development.local -- yarn workspace @techcodes/apollo dev",
